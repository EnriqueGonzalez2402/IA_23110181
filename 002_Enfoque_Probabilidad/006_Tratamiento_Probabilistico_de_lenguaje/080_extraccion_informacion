import spacy

# Cargar modelo de lenguaje en espa√±ol
nlp = spacy.load("es_core_news_sm")

texto = "El presidente Andr√©s Manuel L√≥pez Obrador visit√≥ Oaxaca el 10 de mayo de 2025."

# Analizar texto
doc = nlp(texto)

print("üß† Entidades reconocidas:")
for ent in doc.ents:
    print(f" - {ent.text} ({ent.label_})")